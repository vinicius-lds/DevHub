<AnchorPane fx:id="ap_classe" onContextMenuRequested="#ap_classeContextMenuRequested" onDragDetected="#ap_classeDragDetected" onMouseClicked="#ap_classeMouseClicked" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.proway.controller.projetos.diagramadeclasse.ClasseController">
